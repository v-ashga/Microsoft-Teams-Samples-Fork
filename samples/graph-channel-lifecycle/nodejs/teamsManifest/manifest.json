{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.12/MicrosoftTeams.schema.json",
  "manifestVersion": "1.12",
  "version": "1.0.0",
  "id": "<<BotId>>",
  "packageName": "com.teams.RSC",
  "developer": {
    "name": "Microsoft",
    "websiteUrl": "https://www.microsoft.com",
    "privacyUrl": "https://www.teams.com/privacy",
    "termsOfUseUrl": "https://www.teams.com/termsofuser"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "Channel Lifecycle Node js",
    "full": "Channel Lifecycle sample"
  },
  "description": {
    "short": "Channel Lifecycle Node js",
    "full": "This example show the Channel Lifecycle, which includes channel creation , update and delete"
  },
  "accentColor": "#5558AF",
  "configurableTabs": [
    {
      "configurationUrl": "https://yourngrok.ngrok-free.app/configure",
      "canUpdateConfiguration": true,
      "scopes": [
        "team"
      ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "*.ngrok-free.app"
  ],
 "webApplicationInfo": {
    "id": "<<Your Microsoft App Id>>",
    "resource": "https://RscBasedStoreApp"
  },
  "authorization": {
    "permissions": {
      "resourceSpecific": [
        {
          "name": "TeamsTab.Read.Group",
          "type": "Application"
        },
        {
          "name": "TeamsTab.ReadWrite.Group",
          "type": "Application"
        },
        {
          "name": "ChannelSettings.ReadWrite.Group",
          "type": "Application"
        },
        {
          "name": "Channel.Create.Group",
          "type": "Application"
        },
        {
          "name": "Channel.Delete.Group",
          "type": "Application"
        },
        {
          "name": "TeamsTab.Create.Group",
          "type": "Application"
        },
        {
          "name": "Member.Read.Group",
          "type": "Application"
        }
      ]
    }
  }
}