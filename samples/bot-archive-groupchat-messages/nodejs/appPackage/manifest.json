{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.12/MicrosoftTeams.schema.json",
  "manifestVersion": "1.12",
  "version": "1.0.0",
  "id": "<<MICROSOFT-APP-ID>>",
  "packageName": "com.microsoft.teams.botgroupchatmessages",
  "developer": {
    "name": "Microsoft",
    "websiteUrl": "https://www.microsoft.com",
    "privacyUrl": "https://www.microsoft.com/privacy",
    "termsOfUseUrl": "https://www.microsoft.com/termsofuse"
  },
  "name": {
    "short": "Fetch Groupchat Messages",
    "full": "Fetch groupchat messages with graph api"
  },
  "description": {
    "short": "Fetch groupchat messages.",
    "full": "This sample demos a bot which fetch and archive groupchat messages"
  },
  "icons": {
    "outline": "outline.png",
    "color": "color.png"
  },
  "bots": [
    {
      "botId": "<<MICROSOFT-APP-ID>>",
      "scopes": [
        "team",
        "personal",
        "groupchat"
      ],
      "isNotificationOnly": false,
      "supportsFiles": true
    }
  ],
  "accentColor": "#60A18E",
  "permissions": [ "identity", "messageTeamMembers" ],
  "validDomains": [
    "<<DOMAIN-NAME>>",
    "token.botframework.com"
  ],
  "webApplicationInfo": {
    "id": "<<MICROSOFT-APP-ID>>",
    "resource": "api://botid-<<MICROSOFT-APP-ID>>"
  }
}