{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.12/MicrosoftTeams.schema.json",
    "manifestVersion": "1.12",
    "version": "1.0.0",
    "id": "<<App-Id>>",
    "packageName": "com.teams.calendarmeetings",
    "developer": {
      "name": "Microsoft",
      "websiteUrl": "https://www.microsoft.com",
      "privacyUrl": "https://www.teams.com/privacy",
      "termsOfUseUrl": "https://www.teams.com/termsofuser"
    },
    "icons": {
      "color": "color.png",
      "outline": "outline.png"
    },
    "name": {
      "short": "Create Meetings",
      "full": "Create Meetings"
    },
  "description": {
    "short": "This is an sample application which shows how to create teams meetings in bulk.",
    "full": "This sample application which shows how to create meetings in bulk using Excel file upload method."
  },
    "accentColor": "#5558AF",
    "configurableTabs": [
      {
        "configurationUrl": "https://<<*ngrok>>/configure",
        "canUpdateConfiguration": true,
        "scopes": [
          "team",
          "groupchat"
        ]
      }
    ],
    "permissions": [
      "identity",
      "messageTeamMembers"
    ],
    "validDomains": [
      "<<*ngrok>>"
    ]
  }